page {
  height: 100%;
  background: #F2F2F2;
}

.cashOut_box {
  margin:30rpx;
  min-height: 260px;
  background: #FFFFFF;
  border-radius: 6rpx;
  box-sizing: border-box;
  padding:40rpx 30rpx;
}

.cashOut_title {
  font-size: 28rpx;
  font-weight: 700;
  color: #333333;
  line-height: 40rpx;
}

.cashOut_title_sub {
  font-size: 24rpx;
  font-weight: 400;
  color: #999999;
  line-height: 34rpx;
  margin-top:16rpx;
}

.cashOut_box .value_box {
  display: flex;
  align-items: center;
}

.cashOut_box  .money_icon  {
  font-size: 36rpx;
  font-weight: bold;
  color: #333333;
  line-height: 50rpx;
}

page .van-field__input {
  min-height: 48px;
  font-size: 36rpx;
  font-weight: bold;
  color: #333333;
  line-height: 50rpx;
  border-bottom:1px solid rgba(0, 0, 0, 0.1);
}

.cashOut_box  .input {
  flex: 1;
  flex-shrink: 0;
}

.cashOut_box .charge_tip {
  font-size: 24rpx;
  font-weight: 400;
  color: #666666;
  line-height: 34rpx;
  margin:10rpx 0 0 50rpx;
  background:rgba(236, 244, 255, 1);
  height: 58rpx;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  font-size: 24rpx;
  font-weight: 400;
  color: #666666;
  padding-left: 20rpx;
  position: relative;
}

.cashOut_box .charge_tip .sanjiao {
  width: 0rpx;
  height: 0rpx;
  position: absolute;
  left: 20rpx;
  top: -16rpx;
  border-bottom: 10px solid rgba(236, 244, 255, 1);
  border-left:10px solid transparent;
  border-right:10px solid transparent
}

.cashOut_box  .submit_btn {
  height: 88rpx;
  background: #488FF1;
  border-radius: 6rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 32rpx;
  font-weight: bold;
  color: #FFFFFF;
  margin-top:120rpx;
}

.all_cashOut_box {
  font-size: 24rpx;
  font-weight: 400;
  color: #666666;
  display: flex;
  margin:30rpx 0 0 50rpx;
  box-sizing: border-box;
  padding-left: 20rpx;
}

.all_cashOut_box .all_cashOut_btn {
  color:rgba(72, 143, 241, 1);
  margin-left:20rpx;
}