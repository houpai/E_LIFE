page{
  background-color: #F2F2F2;
  padding-bottom:60rpx;
}

page .van-uploader__upload {
  width: 292rpx!important;
  height: 108rpx!important;
  background: #F2F2F2;
  border-radius: 6rpx;
}

page .van-uploader__preview-image {
  width: 292rpx!important;
  height: 108rpx!important;
}

.cardMaintain_container {
  padding:40rpx 30rpx;
  box-sizing: border-box;
  position: relative;
}

.cardMaintain_container .mask{
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  top:0;
  z-index: 100;
}

.cardMaintain_container .name_logo_box .item_container_box, .cardMaintain_container .store_bg_box .item_container_box{
  box-sizing: border-box;
  padding:30rpx 0 16rpx 0;
  margin:0 30rpx;  
  border-radius: 6rpx;
  border-bottom:1px solid rgba(0, 0, 0, 0.1)
}
.cardMaintain_container .name_logo_box .item_container_box .margin_box {
  margin-top:20rpx;
}

 .name_logo_box, .store_bg_box, .item_container_box {
  background: #FFFFFF;
  border-radius: 6rpx;
  padding-bottom:30rpx;
}

.cardMaintain_container .input_label {
  font-size: 32rpx;
  font-weight: bold;
  color: #333333;
  line-height: 44rpx;
}

.flex_box {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.flex_box .left_box .desc {
  font-size: 24rpx;
  font-weight: 400;
  color: #999999;
  line-height: 34rpx;
  margin-top:12rpx;
}

.cardMaintain_container .store_bg_box{
  margin-top:40rpx;
}

.cardMaintain_container .other_message_box {
  margin-top:40rpx;
}

.edit_btn {
  text-align: center;
  display: flex;
  justify-content: center;
  align-content: center;
}

.edit_btn .flex_btn_box {
  font-size: 32rpx;
  font-weight: 400;
  color: #488FF1;
  display: flex;
  align-content: center;
  justify-content: center;
}

.edit_btn .van-icon {
  font-size:44rpx;
  margin-right: 8rpx;
}

.delete_btn, .edit_btn {
  display: flex;
  align-items: center;
  justify-content: center;
  margin:30rpx;
  height: 88rpx;
  background: #FFFFFF;
  border-radius: 6rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #488FF1;
  line-height: 40rpx;
}