page {
  height: 100%;
  background-color: #fff;
}

.codeTimeOut-container {
  box-sizing: border-box;
  padding: 180rpx 44rpx 0 44rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.codeTimeOut-container .img {
  display: block;
  width: 100%;
  height: 480rpx;
  margin-bottom: 60rpx;
}

.codeTimeOut-container .tip {
  font-size: 32rpx;
  font-weight: 400;
  color: #999999;
  line-height: 44rpx;
}


